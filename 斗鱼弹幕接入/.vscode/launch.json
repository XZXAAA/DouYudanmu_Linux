    "configurations": [

        {
            "name": "(gdb)DOUYU",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/DOUYU",
            "args": ["1126960"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
